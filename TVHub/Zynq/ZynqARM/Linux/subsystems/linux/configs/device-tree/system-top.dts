/dts-v1/;
/include/ "system-conf.dtsi"
/ {
};

	&gem0 {
		phy-handle = <&phy0>;
		phy-mode = "rgmii-id";
		
		mdio {
			#address-cells = <1>;
			#size-cells = <0>;
			phy0: phy@0 {
				compatible = "marvell,88e1510";
				device_type = "ethernet-phy";
				reg = <0x0>;
				marvell,reg-init = <3 16 0xff00 0x1e 3 17 0xfff0 0x00>;
			};
		};
	};

	&qspi {
		flash0: flash@0 {
	       	       compatible = "micron,n25q128a13";
		};
	};

	&uart_stm {
    		interrupt-parent = <&intc>;
    		interrupts = <0 31 1>;
     		device_type = "serial";
	};

	&spi_pll {
	    interrupt-parent = <&intc>;
	    interrupts = <0 32 1>;
	    config_mux:spidev@0 {
		compatible = "spidev", "linux,spidev";
		reg = <0>;
		spi-max-frequency = <4000000>;
	    };    
	};
